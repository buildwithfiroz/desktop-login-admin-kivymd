MDScreen:
    name: 'view_screen' 

    MD3Card:
        id: card 
        padding: ('10dp', '10dp', '10dp', '10dp')
        size_hint: None, None
        size: '1100dp','1100dp'
        height: '100dp'
        opacity: 1
        pos_hint: {'center_x': .5,'center_y': .9}
        elevation: 1

        MDRelativeLayout:
            GridLayout:
                cols: 4
                size_hint: None, None
                size: self.minimum_size
                pos_hint: {'center_x': 1.79, 'center_y': 0.5}
                spacing: '9dp'

                MDIconButton:
                    icon: "message"
                    theme_text_color: "Custom"
                    color: 0.5,0.5,0.5,0.5

                MDIconButton:
                    icon: "bell"
                    theme_text_color: "Custom"
                    color: 0.5,0.5,0.5,0.5
                    badge_text: "1"  # Display a badge indicating one unread message

                MDIconButton:
                    icon: "logout"
                    theme_text_color: "Custom"
                    opacity:0.7
                    on_release: app.switch_to_login()

                AsyncImage:
                    source: 'img/wans.png'
                    pos_hint: {'center_x': 0.84,'center_y': .5}
                    size_hint: None, None  # Disables automatic resizing based on parent


        MDRelativeLayout:


            AsyncImage:
                source: 'img/logo.png'
                pos_hint: {'center_x': -0.92,'center_y': .5}
                size_hint: None, None  # Disables automatic resizing based on parent

            


            MDLabel:
                id: label
                text: 'Admin Panel'
                theme_text_color: 'Custom'
                pos_hint: {"center_x": -0.357, "center_y": .38}
                text_color: 0.5 , 0.5 , 0.5 , 1
                bold: True
                font_size: "11sp"
                opacity: 0.8

            MDLabel:
                id: label
                text: 'NEXGENO'
                theme_text_color: 'Custom'
                pos_hint: {"center_x": -0.37, "center_y": .6}
                text_color: 1 , 1 , 1 , 1
                bold: True
                font_style: 'Body1'
                font_size: "20sp"
                opacity: 0.9

    MD3Card:
        id: card 
        padding: ('10dp', '10dp', '10dp', '10dp')
        size_hint: None, None
        size: '100dp','100dp'
        height: '590dp'
        opacity: 1
        pos_hint: {'center_x': .083,'center_y': .42}
        elevation: 1






    MD3Card:
        id: card 
        padding: ('10dp', '10dp', '10dp', '10dp')
        size_hint: None, None
        size: '980dp','980dp'
        height: '540dp'
        opacity: 1
        pos_hint: {'center_x': .54,'center_y': .45}
        elevation: 1

        MDRelativeLayout:

            MDLabel:
                id: label
                text: 'View User'
                theme_text_color: 'Custom'
                pos_hint: {'center_x': 0.405,'center_y': 0.99}
                text_color: 0.5 , 0.5 , 0.5 , 1
                bold: True
                font_size: "11sp"
                opacity: 0.8

            MDIconButton:
                icon: "home"
                icon_size: "50dp"
                pos_hint: {'center_x': -0.07,'center_y': 0.86}
                on_press:app.switch_to_admin()


            MDFloatingActionButton:
                icon: "eye-off"
                elevation_normal: 15
                pos_hint: {'center_x': -0.07,'center_y': 0.69}
                md_bg_color: app.theme_cls.primary_color  
                text_color: 1,1,1, 0.8 
                on_press: app.refresh_page()

            MDIconButton:
                icon: "plus"
                icon_size: "50dp"
                pos_hint: {'center_x': -0.07,'center_y': 0.54}
                on_release: app.switch_to_add_sceen()

            MDIconButton:
                icon: "calendar"
                icon_size: "50dp"
                pos_hint: {'center_x': -0.07,'center_y': 0.38}
                on_press:app.switch_to_view_screen()
                on_release: app.show_date_picker()

            MDLabel:
                id: label_team_member
                text: 'Team Members'
                theme_text_color: 'Custom'
                pos_hint: {"center_x": .54, "center_y": .96}
                text_color: 1,1,1,1
                bold: True
                font_size: "14sp"
                opacity: 0.9

            MDLabel:
                id: new_total_value
                text: '1'
                theme_text_color: 'Custom'
                pos_hint: {"center_x": .64, "center_y": .98}
                text_color: 1,1,1,1
                bold: True
                font_size: "10sp"
                opacity: 0.8

            MDIconButton:
                id:mg_to_search
                icon: "magnify"
                theme_text_color: "Custom"
                color: 0.5,0.5,0.5,0.5
                pos_hint: {"center_x": .92, "center_y": .96}
                opacity:0.8
                on_release:app.change()

            

            MDIconButton:
                icon:'refresh'
                theme_text_color: "Custom"
                color: 0.5,0.5,0.5,0.5
                opacity:0.8
                pos_hint: {"center_x": .98, "center_y": .96}

                on_release: app.refresh_page()



            MDLabel:
                id: id
                text: 'ID'
                theme_text_color: 'Custom'
                pos_hint: {"center_x": .55, "center_y": .83}
                text_color: 0.5 , 0.5 , 0.5 , 1
                bold: True
                font_size: "11sp"
                opacity: 0.8

            MDLabel:
                id: UserName
