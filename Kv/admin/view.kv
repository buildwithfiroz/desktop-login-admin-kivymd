                theme_text_color: 'Custom'
                pos_hint: {"center_x": 1.1, "center_y": .83}
                text_color: 0.5 , 0.5 , 0.5 , 1
                bold: True
                font_size: "11sp"
                opacity: 0.8

            MDLabel:
                id: Time
